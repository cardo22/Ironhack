<h1><%= @my_project.name %></h1>

<ul>
	<% @my_entries.each do |entry| %>
		<li>
			<%= entry.hours %>
			<%= entry.minutes %>
			<%= entry.date %>

			<a href="/projects/<%= @my_project.id %>/time_entries/<%= entry.id %>/edit">Edit</a>
			<%= link_to("DELETE THIS ENTRY", project_time_entry_path(@my_project, entry)) %>		
		</li>
	<% end %>
</ul>